void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  pinMode(13, OUTPUT);
  pinMode(7, INPUT);
  


}

void loop() {
  // put your main code here, to run repeatedly:
  int value = digitalRead(7);
  Serial.println(value);

  if (value==1){
    digitalWrite(13, HIGH);
  }
  else{
    digitalWrite(13, LOW);
  }

}
